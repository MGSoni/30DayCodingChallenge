public class Solution {
    // DO NOT MODIFY THE ARGUMENTS WITH "final" PREFIX. IT IS READ ONLY
    public int maxSubArray(final int[] A) {

        int currSum =0;
        int maxSum=Integer.MIN_VALUE;

        for(int x:A){
            currSum+=x;
            if(currSum>maxSum){
                maxSum=currSum;
            }

            if(currSum<0){
                currSum=0;
            }

        }

        return maxSum;
    }
}
